package templates

templ Home(name string) {
<!DOCTYPE html>
<html>

<head>
	<title>Go Echo Templ { name }</title>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<link rel="stylesheet" href="/public/styles.css" />
</head>

<body>
	<h1 class="text-clifford underline">Hello, { name }</h1>
</body>

</html>
}
